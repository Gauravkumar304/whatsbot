  background-color: #a7a7a7;
  cursor: pointer;
  transition-duration: 0.5s;
}

.check-smatest:hover,
.check-smatest:focus {
  box-shadow: inset 6.5em 0 0 0 var(--hover);
}

.check-smatest::before {
  display: block;
  content: "";
  border-radius: 50%;
  height: 12px;
  width: 12px;
  margin-left: -1px;
  margin-top: -1px;
